{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "10mb"
        }
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/.*",
        "dest": "vite.config.js"
      },
      {
        "src": "/.*",
        "dest": "public/index.html"
      }
    ]
  }
  